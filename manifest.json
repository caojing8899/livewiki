{
  "manifest_version": 2,

  "name": "LiveWiki",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.wikipedia.org/*"],
      "js": ["wiki_crawler.js"],
      "css": ["wiki_crawler.css"]
    }
  ]
}